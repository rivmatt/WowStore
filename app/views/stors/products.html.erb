<h1>Stors products</h1>
<p>Find me in app/views/stors/products.html.erb</p>


<table>
  <thead>
    <tr>
      <th width="150">Nombre </th>
      <th>Descripcion</th>
      <th width="100">Precio</th>
      <th width="150">Visible</th>
      <th width="150">tienda</th>
      <th width="150">Creacion</th>
      <th width="150">Actualzacion</th>

    </tr>
  </thead>
  <tbody>
  	<% @products.each do |product| %>
    <tr>
      <!-- <td><% product.name %></td> -->
      <td><%= link_to "#{product.name}", trends_path(product) %></td>
      <td><%= product.description %></td>
      <td><%= product.price %></td>
      <td><%= product.visible %></td>
      <td><%= product.store_id %></td>
      <td><%= product.created_at %></td>
      <td><%= product.updated_at %></td>
    </tr>
    <% end %>
  </tbody>
</table>


